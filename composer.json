{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-snappy": "^0.4.0",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.7",
        "h4cc/wkhtmltopdf-amd64": "0.12.2.1",
        "h4cc/wkhtmltopdf-i386": "0.12.2.1",
        "hisorange/browser-detect": "^3.1",
        "intervention/image": "^2.4",
        "knplabs/knp-snappy": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "mariuzzo/laravel-js-localization": "^1.4",
        "mcamara/laravel-localization": "^1.3",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "risul/laravel-like-comment": "^2.0",
        "uxweb/sweet-alert": "^1.4",
        "yajra/laravel-datatables-buttons": "3.0",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
        ,
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
